<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.pojo.BangThamSo" table="bangthamso" schema="quanlystk">
        <id name="maThamSo">
            <column name="MaThamSo" sql-type="int"/>
        </id>
        <property name="tenThamSo">
            <column name="TenThamSo" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="kieu">
            <column name="Kieu" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="giaTri">
            <column name="GiaTri" sql-type="varchar(255)"/>
        </property>
        <property name="tinhTrang">
            <column name="TinhTrang" sql-type="boolean"/>
        </property>
    </class>
</hibernate-mapping>