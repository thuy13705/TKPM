<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.pojo.LoaiSTK" table="loaistk" schema="quanlystk">
        <id name="loaiSo">
            <column name="LoaiSo" sql-type="int"/>
        </id>
        <property name="tenLoai">
            <column name="TenLoai" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="laiSuat">
            <column name="LaiSuat" sql-type="float(5,2)" precision="5" scale="2"/>
        </property>
        <property name="tinhTrang">
            <column name="TinhTrang" sql-type="int" />
        </property>
    </class>
</hibernate-mapping>